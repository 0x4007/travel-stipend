{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "ignorePaths": [
    "**/*.json",
    "**/*.css",
    "node_modules",
    "**/*.log",
    "**/*.csv",
    "src/test-date-selection.ts",
    "src/test-destination-input.ts",
    "src/test-google-flights.ts",
    "src/test-specific-destination-input.ts"
  ],
  "useGitignore": true,
  "language": "en",
  "words": [
    "commitlint",
    "dataurl",
    "Devcon",
    "devpool",
    "Incheon",
    "outdir",
    "serpapi",
    "servedir"
  ],
  "dictionaries": [
    "typescript",
    "node",
    "software-terms"
  ],
  "import": [
    "@cspell/dict-typescript/cspell-ext.json",
    "@cspell/dict-node/cspell-ext.json",
    "@cspell/dict-software-terms"
  ],
  "ignoreRegExpList": [
    "[0-9a-fA-F]{6}"
  ]
}
